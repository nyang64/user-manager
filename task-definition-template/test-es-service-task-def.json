{
  "executionRoleArn": "arn:aws:iam::488323030461:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/test-es-ecs",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 5000,
          "protocol": "tcp",
          "containerPort": 5000
        }
      ],
      "cpu": 0,
      "environment": [
        {
          "name": "APP_LINK",
          "value": "http://applink"
        },
        {
          "name": "POSTGRES_DB_HOST",
          "value": "test-es-db.caneampnjecs.us-west-1.rds.amazonaws.com"
        },
        {
          "name": "POSTGRES_DB_NAME",
          "value": "testes"
        },
        {
          "name": "POSTGRES_DB_PORT",
          "value": "5432"
        },
        {
          "name": "SMTP_FROM",
          "value": "elementapp@elementsci.com"
        },
        {
          "name": "SMTP_PORT",
          "value": "587"
        },
        {
          "name": "SMTP_SERVER",
          "value": "email-smtp.us-west-1.amazonaws.com"
        }
      ],
      "secrets": [
        {
          "valueFrom": "arn:aws:secretsmanager:us-west-1:488323030461:secret:user-manager-secrets-pd4Rwt",
          "name": "user-manager-secrets"
        }
      ],
      "volumesFrom": [],
      "image": "$IMAGE_NAME",
      "essential": true,
      "dockerLabels": {
        "Org": "Infostretch"
      },
      "name": "test-es-api"
    }
  ],
  "placementConstraints": [],
  "memory": "2048",
  "taskRoleArn": "arn:aws:iam::488323030461:role/ECS-Managed-Policy",
  "compatibilities": [
    "EC2",
    "FARGATE"
  ],
  "taskDefinitionArn": "arn:aws:ecs:us-west-1:488323030461:task-definition/test-es-ecs:34",
  "family": "test-es-ecs",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "volumes": []
}
