{
  "executionRoleArn": "arn:aws:iam::488323030461:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/$BITBUCKET_DEPLOYMENT_ENVIRONMENT-user-manager",
          "awslogs-region": "$AWS_DEFAULT_REGION",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 5000,
          "protocol": "tcp",
          "containerPort": 5000
        }
      ],
      "cpu": 0,
      "environment": [
        {
          "name": "APP_LINK",
          "value": "http://applink"
        },
        {
          "name": "POSTGRES_DB_HOST",
          "value": "$DB_URL"
        },
        {
          "name": "POSTGRES_DB_NAME",
          "value": "postgres"
        },
        {
          "name": "POSTGRES_DB_PORT",
          "value": "5432"
        },
        {
          "name": "OTP_EXPIRATION_TIME_HOURS",
          "value": "24"
        },
        {
          "name": "OTP_EXPIRATION_TIME_MINUTES",
          "value": "0"
        },
        {
          "name": "OTP_LIMIT",
          "value": "7"
        },
        {
          "name": "OTP_LIMIT_HOURS",
          "value": "1"
        },
        {
          "name": "OTP_LIMIT_MINUTES",
          "value": "0"
        },
        {
          "name": "SMTP_FROM",
          "value": "elementapp@elementsci.com"
        },
        {
          "name": "SMTP_PORT",
          "value": "587"
        },
        {
          "name": "SMTP_SERVER",
          "value": "email-smtp.$AWS_DEFAULT_REGION.amazonaws.com"
        },
	{
          "name": "OTP_EXPIRATION_TIME_HOURS",
          "value": "24"
        },
	{
          "name": "OTP_EXPIRATION_TIME_MINUTES",
          "value": "0"
        },
	{
          "name": "OTP_LIMIT",
          "value": "7"
        },
	{
          "name": "OTP_LIMIT_HOURS",
          "value": "1"
        },
	{
          "name": "OTP_LIMIT_MINUTES",
          "value": "0"
        }
      ],
      "secrets": [
        {
          "valueFrom": "$SM_ARN",
          "name": "user-manager-secrets"
        }
      ],
      "volumesFrom": [],
      "image": "$IMAGE_NAME",
      "essential": true,
      "dockerLabels": {
        "Org": "Infostretch"
      },
      "name": "$BITBUCKET_DEPLOYMENT_ENVIRONMENT-es-container"
    }
  ],
  "placementConstraints": [],
  "memory": "1024",
  "taskRoleArn": "arn:aws:iam::488323030461:role/ECS-Managed-Policy",
  "family": "$BITBUCKET_DEPLOYMENT_ENVIRONMENT-user-manager",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "512",
  "volumes": []
}