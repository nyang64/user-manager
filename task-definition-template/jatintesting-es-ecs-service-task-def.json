{
  "executionRoleArn": "arn:aws:iam::488323030461:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/jatintesting-user-manager-task-def",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 5000,
          "protocol": "tcp",
          "containerPort": 5000
        }
      ],
      "cpu": 0,
      "environment": [
        {
          "name": "APP_LINK",
          "value": "http://applink"
        },
        {
          "name": "POSTGRES_DB_HOST",
          "value": "jatintesting-es-db.cypfku5dqbr4.us-east-1.rds.amazonaws.com"
        },
        {
          "name": "POSTGRES_DB_NAME",
          "value": "postgres"
        },
        {
          "name": "POSTGRES_DB_PORT",
          "value": "5432"
        },
        {
          "name": "SMTP_FROM",
          "value": "elementapp@elementsci.com"
        },
        {
          "name": "SMTP_PORT",
          "value": "587"
        },
        {
          "name": "SMTP_SERVER",
          "value": "email-smtp.us-east-1.amazonaws.com"
        }
      ],
      "secrets": [
        {
          "valueFrom": "arn:aws:secretsmanager:us-east-1:488323030461:secret:jatintesting-es-usr-mgr-secret2-dYjFWi",
          "name": "user-manager-secrets"
        }
      ],
      "volumesFrom": [],
      "image": "$IMAGE_NAME",
      "essential": true,
      "dockerLabels": {
        "Org": "Infostretch"
      },
      "name": "jatintesting-es-container"
    }
  ],
  "placementConstraints": [],
  "memory": "2048",
  "taskRoleArn": "arn:aws:iam::488323030461:role/ECS-Managed-Policy",
  "family": "jatintesting-user-manager-task-def",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "volumes": []
}

